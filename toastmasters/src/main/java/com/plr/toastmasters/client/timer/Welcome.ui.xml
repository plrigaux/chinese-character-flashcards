<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.panel {
			margin: 0px auto;
		}
	</ui:style>
	
	<g:HTMLPanel>
		<g:VerticalPanel styleName="{style.panel}" spacing="10" horizontalAlignment="ALIGN_CENTER">
			<g:Button text="Table topics" ui:field="tableTopics" width="160px" />
			<g:Button text="Prepared speach" ui:field="preparedSpeach" width="160px" />
			<g:Button text="Evaluation" ui:field="evaluation" width="160px" />
			<g:Button text="Other" ui:field="other" width="160px" />
			<g:Button text="Custom" ui:field="custom" width="160px" />

		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 