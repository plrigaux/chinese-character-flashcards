<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default">

	<ui:style>
		.panel {
			margin: 0px auto;
		}
	</ui:style>



	<g:DockLayoutPanel unit='EM'>
		<g:north size='5'>
			<g:Label>Top</g:Label>
		</g:north>
		<g:center>
			<g:ScrollPanel>
				<g:HTMLPanel ui:field="centerPanel">

				</g:HTMLPanel>
			</g:ScrollPanel>
		</g:center>
		<g:west size='10'>
			<g:VerticalPanel styleName="{style.panel}" spacing="10">
				<g:Button text="Login" ui:field="login" />
				<g:Button text="Process Instances" ui:field="processInstances" />
				<g:Button text="Processes" ui:field="processDefinitions" />
				<g:Button text="About" ui:field="about" />
			</g:VerticalPanel>
		</g:west>
	</g:DockLayoutPanel>
</ui:UiBinder> 