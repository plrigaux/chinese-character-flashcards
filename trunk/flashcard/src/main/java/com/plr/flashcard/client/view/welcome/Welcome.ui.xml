<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:com.plr.flashcard.client.view.welcome">

	<ui:style>
		.panel {
			margin: 0px auto;
		}
	</ui:style>
	<g:VerticalPanel styleName="{style.panel}" spacing="10"
		horizontalAlignment="ALIGN_CENTER">
		<w:SVGButton text="Flashcards" ui:field="flashcards" />
		<w:SVGButton text="Guess" ui:field="guesser" />
		<w:SVGButton text="Char Browser" ui:field="browser" />

		<w:SVGButton text="Help" ui:field="help" />
		<w:SVGButton text="About" ui:field="about" />

	</g:VerticalPanel>
</ui:UiBinder> 