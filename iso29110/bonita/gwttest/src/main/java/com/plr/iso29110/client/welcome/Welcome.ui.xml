<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:com.plr.iso29110.client.welcome"
 ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
    ui:generateLocales="default"
>

	<ui:style>
		.panel {
			margin: 0px auto;
		}
	</ui:style>
	
	<g:HTMLPanel>
		<g:VerticalPanel styleName="{style.panel}" spacing="10" horizontalAlignment="ALIGN_CENTER">
			<w:CustomButton text="Submit Bug" ui:field="submitBug" width="160px" />
			<w:CustomButton text="List of bugs to review" ui:field="bugsToReview" width="160px"/>
		
			<w:CustomButton text="About" ui:field="about" width="160px"/>
			
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 